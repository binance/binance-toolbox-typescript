{
    "compilerOptions": {
        "target": "es2021",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
    ]
}